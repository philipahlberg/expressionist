{
  "name": "trailblazer",
  "version": "4.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/philipahlberg/expressionist.git",
  "author": "philipahlberg <23071488+philipahlberg@users.noreply.github.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run test:watch",
    "dev": "npm run test:watch",
    "test": "karma start scripts/karma.prod.js",
    "test:watch": "karma start scripts/karma.dev.js",
    "build": "npm run build:types && rollup -c scripts/rollup.prod.js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:watch": "rollup -c scripts/rollup.dev.js -w",
    "docs": "npm run docs:build && npm run docs:serve",
    "docs:build": "typedoc src/ --exclude \"**/constants.ts\" --out docs/",
    "docs:serve": "http-server ./docs",
    "perf": "node --experimental-modules --allow-natives-syntax perf/optimizations.mjs",
    "bench": "node --experimental-modules perf/benchmark.mjs"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "http-server": "^0.11.1",
    "karma": "^3.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^5.2.0",
    "path-to-regexp": "^2.4.0",
    "rollup": "^0.66.6",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-typescript": "^1.0.0",
    "tslib": "^1.9.3",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.4"
  }
}
